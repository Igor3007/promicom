extends ../layouts/default
include ../helpers/mixins
include ../../blocks/components/breadcrumbs/breadcrumbs.pug
include ../../blocks/components/form/form.pug
include ../../blocks/components/checkbox/checkbox.pug
include ../../blocks/components/headers/headers.pug
include ../../blocks/components/table/table.pug
include ../../blocks/components/popup/popup.pug

block variables
    - const title = "Проекты - Vako"

block styles
    link(rel="stylesheet" href="static/styles/vako.css")

block content

    .page-breadcrumbs
        .container
            +breadcrumb([
                {
                    link: "/projects",
                    text: "Проекты",
                },
                {
                    link: "/",
                    text: "Vako",
                    isActive: true,
                }
            ], 'wow experience')


    include ../../blocks/modules/vakos/vakos
    include ../../blocks/modules/steps/steps-vakos

    include ../../blocks/modules/home/home-form/home-form

    +callbackPopup()

block scripts

    script(src="static/js/vako.js")
    script(src="static/js/main.js")