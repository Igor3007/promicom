                .service-licenses__tabs-pane.service-tabs__pane.is-active(data-tab="license")
                    .service-licenses__items
                        each item in list.licenses
                            .service-licenses__item
                                .service-licenses__item-image
                                    picture
                                        source(srcset=item.image+".webp" type="image/webp")
                                        img.lazyload(loading="lazy" data-src=item.image + ".png" width="69" height="84")
                                .service-licenses__item-head
                                    .service-licenses__item-name.section-h4=item.title
                                    .service-licenses__item-price=item.price
                                if item.desc
                                    .service-licenses__item-desc=item.desc
                                if item.features
                                    ul.service-licenses__item-features
                                        each i in item.features
                                            li=i
                                .service-licenses__item-actions
                                    a(href="#").btn.btn-secondary Купить
                .service-licenses__tabs-pane.service-tabs__pane(data-tab="renew")
                    .service-licenses__items
                        each item in list.renew
                            .service-licenses__item
